    "ng": "ng",
    "start": "ng serve --port 44444 --proxy-config proxy.conf.js --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
